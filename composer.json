{
    "name": "reesmcivor/laravel-comments",
    "description": "Laravel Comments",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rees McIvor",
            "email": "reesmcivor@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0",
        "livewire/livewire": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "ReesMcIvor\\Comments\\": "src/",
            "ReesMcIvor\\Comments\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ReesMcIvor\\Comments\\CommentsPackageServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
